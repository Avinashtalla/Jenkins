pipeline {
    agent any
    stages {
        stage('git checkout') {
            steps {
                git branch: 'main', url 'https://github.com/Avinashtalla/example-voting-app.git'
            }
        }
        stage('Unit Testing') {
            steps {
                cd example-voting-app.git/
                cd vote/
                sh 'mvn test'
            }
        }
    }
}
