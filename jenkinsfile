pipeline {

    agent any

    stages {

        stage('git checkout'){

            steps {
                git branch: 'main', changelog: false, poll: false, url: 'https://github.com/Avinashtalla/Jenkins.git'
            }
        
        }
        stage('Unit Testing') {
            
            steps {
                sh 'mvn test'
            }
        }
    }
}
